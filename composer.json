{
	"name": "fuzz/magic-box",
	"description": "A magical implementation of Laravel's Eloquent models as injectable, masked resource respositories.",
	"require": {
		"illuminate/database": "~5.0",
		"fuzz/laravel-data": "5.0.x-dev"
	},
	"require-dev": {
		"orchestra/testbench": "3.0.*",
	  	"phpmd/phpmd" : "@stable",
	  	"squizlabs/php_codesniffer": "2.*",
	  	"sebastian/phpcpd": "*",
		"pdepend/pdepend" : "@stable",
		"phploc/phploc": "*",
		"phpunit/phpunit": "4.7.*",
		"theseer/phpdox": "0.8.*",
	  	"phpunit/php-invoker": "1.*"
	},
	"authors": [
		{
			"name": "Fuzz Productions",
			"email": "fuzzweb@fuzzproductions.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@gitlab.fuzzhq.com:web-modules/laravel-data.git"
		},
		{
			"type": "composer",
			"url": "https://satis.fuzzhq.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Fuzz\\MagicBox\\": "src/",
			"Fuzz\\MagicBox\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev"
}
